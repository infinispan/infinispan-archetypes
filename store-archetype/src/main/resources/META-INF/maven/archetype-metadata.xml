<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="custom-cache-store"
                      xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <modules>
      <module id="cache-loader" dir="cache-loader" name="cache-loader">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
      <module id="cache-writer" dir="cache-writer" name="cache-writer">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
      <module id="advanced-cache-loader" dir="advanced-cache-loader" name="advanced-cache-loader">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
      <module id="advanced-cache-writer" dir="advanced-cache-writer" name="advanced-cache-writer">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
      <module id="advanced-load-write-store" dir="advanced-load-write-store" name="advanced-load-write-store">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
      <module id="external-store" dir="external-store" name="external-store">
         <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
               <directory>src/main/java</directory>
               <includes>
                  <include>**/*.java</include>
               </includes>
            </fileSet>
         </fileSets>
      </module>
   </modules>
</archetype-descriptor>
